<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Android test suite on Android Real Phone">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="RealDevice"/>
    <parameter name="deviceName" value="Galaxy S9"/>
    <parameter name="version" value="8.0.0"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
           <class name="HomePageMenu.MenuTest"></class>
            <class name="HomePageMenu.MenuPage"></class>
            <class name="HomePageMenu.NotificationsTest"></class>
        </classes>
    </test>
</suite>
